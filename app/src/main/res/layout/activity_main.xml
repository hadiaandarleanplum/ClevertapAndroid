<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/btnTriggerJourney"
        android:layout_width="165dp"
        android:layout_height="46dp"
        android:layout_marginStart="127dp"
        android:layout_marginTop="92dp"
        android:layout_marginEnd="127dp"
        android:text="Sync Variables"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnLoginUser1" />

    <Button
        android:id="@+id/btnLoginUser2"
        android:layout_width="176dp"
        android:layout_height="41dp"
        android:layout_marginStart="127dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="127dp"
        android:text="Login as Rose Doe"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.526"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnLoginUser1" />

    <Button
        android:id="@+id/btnLoginUser1"
        android:layout_width="208dp"
        android:layout_height="40dp"
        android:layout_marginStart="127dp"
        android:layout_marginTop="143dp"
        android:layout_marginEnd="127dp"
        android:text="Login as Jack Montana6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnAppInbox"
        android:layout_width="179dp"
        android:layout_height="44dp"
        android:layout_marginStart="156dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="161dp"
        android:text="Open App Inbox"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.47"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnLoginUser2" />

    <TextView
        android:id="@+id/varView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="textView"
        tools:layout_editor_absoluteX="178dp"
        tools:layout_editor_absoluteY="486dp" />

    <Button
        android:id="@+id/btnLogout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Logout and clear cache"
        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="321dp" />

</androidx.constraintlayout.widget.ConstraintLayout>